{
  "name": "usta",
  "version": "0.0.0",
  "author": "Davit Martirosyan <davo.martirosyan001@gmail.com>",
  "private": true,
  "license": "MIT",
  "engines": {
    "node": ">=18.0.0"
  },
  "scripts": {
    "build:prod": "tsc -p tsconfig.build.json",
    "postbuild:prod": "copyfiles --up 1 src/**/*.json dist",
    "start:dev": "ts-node src/main.ts",
    "start:prod": "node dist/main.js",
    "new": "hygen new",
    "watch:dev": "ts-node-dev src/main.ts",
    "debug:dev": "cross-env TS_NODE_CACHE=false ts-node-dev --inspect --ignore '/^src/.*\\.spec\\.ts$/' src/main.ts",
    "webpack": "webpack --config webpack.config.js --progress",
    "lint": "eslint . --ext .ts",
    "lint:fix": "eslint --fix . --ext .ts"
  },
  "dependencies": {
    "@nestjs/common": "^11.0.20",
    "@nestjs/config": "^4.0.2",
    "@nestjs/core": "^11.0.20",
    "@nestjs/platform-express": "^11.0.20",
    "@nestjs/swagger": "^7.3.1",
    "blurhash": "^2.0.5",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.14.1",
    "compression": "^1.8.0",
    "express": "^4.21.2",
    "helmet": "^8.1.0",
    "lodash": "^4.17.21",
    "mime-types": "^3.0.1",
    "morgan": "^1.10.0",
    "reflect-metadata": "^0.2.2",
    "rxjs": "^7.8.2",
    "sharp": "^0.34.1",
    "ts-node": "^10.9.2"
  },
  "devDependencies": {
    "@types/compression": "^1",
    "@types/express": "^5.0.1",
    "@types/lodash": "^4.17.16",
    "@types/luxon": "^3.6.2",
    "@types/mime-types": "^2.1.4",
    "@types/morgan": "^1.9.9",
    "@types/node": "^22.14.1",
    "@typescript-eslint/eslint-plugin": "^7.12.0",
    "@typescript-eslint/parser": "^7.12.0",
    "cross-env": "^7.0.3",
    "eslint": "8.57.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-canonical": "^4.18.1",
    "eslint-plugin-deprecation": "^2.0.0",
    "eslint-plugin-import": "^2.29.1",
    "eslint-plugin-n": "^17.8.1",
    "eslint-plugin-prettier": "^5.1.3",
    "eslint-plugin-promise": "^6.2.0",
    "eslint-plugin-simple-import-sort": "^12.1.0",
    "eslint-plugin-sonarjs": "^0.25.1",
    "eslint-plugin-unicorn": "^53.0.0",
    "husky": "^9.1.7",
    "lint-staged": "~15.5.1",
    "prettier": "^3.5.3",
    "typescript": "^5.8.3"
  },
  "lint-staged": {
    "*.ts": [
      "eslint --fix"
    ]
  },
  "packageManager": "yarn@4.5.0"
}
